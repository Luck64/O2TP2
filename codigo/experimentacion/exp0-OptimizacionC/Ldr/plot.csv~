#Optimizacion del compilador - LDR
set terminal png 
#set title "LDR"
set xlabel "Cantidad de pixeles"
set ylabel "Ciclo de Clocs" 
set grid
set output 'ldrOptimizacion.png'
set style data linespoints 
set key under box
plot "ldr_c_O0.out" using 1:2 lt 10 lw 2 smooth bezier title '-O0', "ldr_c_O3.out" using 1:2 lt 7 lw 2 smooth bezier title '-O3'

